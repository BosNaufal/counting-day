{
  "name": "counting-day",
  "version": "0.0.6",
  "description": "Mini library to count your day",
  "main": "./dist/counting-day.min.js",
  "scripts": {
    "start": "webpack-dev-server --config ./webpack/webpack.config.js --inline --hot",
    "build": "webpack --config ./webpack/webpack-prod.config.js -p",
    "bundle": "webpack --config ./webpack/webpack-bundle.config.js && webpack --config ./webpack/webpack-bundle.config.js -p",
    "test": "karma start karma.conf.js",
    "prepublish": "npm run build && npm run bundle"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BosNaufal/counting-day.git"
  },
